

<div class="container">
	<h1>Users</h1>
	

	<table class="table">
		<thead class="thead thead-default">
			<tr>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Email</th>
				<th>Created at</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
			<tr>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.email %></td>
				<td><%= time_ago_in_words user.created_at %></td>
			</tr>
			<% end %>
		</tbody>
	</table>

</div>